{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "klu-ai",
  "title": "Klu",
  "description": "",
  "icon": "spark.png",
  "author": "abielzulio",
  "license": "MIT",
  "commands": [
    {
      "name": "search-actions",
      "title": "Search Actions",
